alias gchlog='git log --all --decorate=short   | grep -zo '\''tag: [^\)]*\|\+[ ]\?...-[^\[]*\|\+[ ]\?....-[^\[]*\|\[CHANGELOG\][^\[]*\[\/CHANGELOG\]'\''   | tr '\''\0'\'' '\''\n'\''   | grep -v CHANGELOG   | sed '\''s/tag: \([^,]*\).*/Versione \1/g'\''   | sed '\''s/^[ ]*+/\t\+/g'\''   | sed '\''s/^[ ]*-/\t\-/g'\''   | sed '\''s/\+[ ]*\([^ ]\)/\+ \1/g'\''   | sed '\''s/^[ ]*\([^ ]\)/\1/g'\''   | sed '\''s/^[ ][ ]*\n//g'\''   | grep -v "^    $" | tac'
